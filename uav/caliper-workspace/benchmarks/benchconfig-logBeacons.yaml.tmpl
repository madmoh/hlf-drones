test:
  name: abyssar-benchmark
  workers:
    number: $WORKERS
  rounds:
  - label: logBeacons-$TPS
    txDuration: $TX_DURATION
    rateControl:
      type: fixed-rate
      opts:
        tps: $TPS
    workload:
      module: workload/logBeacons.js
      arguments:
        operatorsPerWorker: 8
        flightsPerOperator: 1
        beaconsPerLog: 120
        violationRate: 0.005
        chaincodeName: abyssarCC
